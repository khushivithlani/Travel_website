        var events=data.events.sort(bydate);;
        var temp="";
        var month_arr = ["","Jan", "Feb", "Mar", "Apr", "May", "June",
        "July", "Aug", "Sep", "Oct", "Nov", "Dec"];

        if(typeof events != 'undefined'){
              for(i=0;i<events.length;i++)
            {
            var dt=events[i].startDate.split("-");
            var dte=events[i].endDate.split("-");
            var startday=parseInt(dt[2]);
            var startmonth=month_arr[parseInt(dt[1],10)];
            var endday=parseInt(dte[2]);
            var endmonth=month_arr[parseInt(dte[1],10)];


            //temp+='<li class="clearfix"><img width="80" height="61" src="'+events[i].url+'"/><h4><a href="#">'+events[i].name+'</a></h4><span class="meta">'+startday+' '+startmonth+'  '+endday+' '+endmonth+'</span></li>';
            if(startday==endday&&startmonth==endmonth)
            {
            temp+='<li class="clearfix"><h3><a>'+events[i].name+'</a></h3><span class="meta">'+startday+' '+startmonth+'</span></li>';
            }
            else
            {
            temp+='<li class="clearfix"><h3><a>'+events[i].name+'</a></h3><span class="meta">'+startday+' '+startmonth+'  '+endday+' '+endmonth+'</span></li>';
            }
            }
        }

        if(temp!="") {

