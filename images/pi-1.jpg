function convert(num) {
  if(show_temp=="c")
  {return num+"&degC";}
  else
  {
  return parseInt(num*1.8+32)+"&degF";
  }

}
function f_weather_date(dte)
{

dte=dte.split("-");
day_s=dte[2]
if(day_s.length==1) {day_s="0"+day_s;}
month_s=dte[1];
if(month_s.length==1){month_s="0"+month_s;}
year_s=dte[0];
dte=year_s+"-"+month_s+"-"+day_s;
return dte;
}
function weather_w(data,type){
var w_block=$(".weather-info");
var month = ["January", "February", "March", "April", "May", "June",
"July", "August", "September", "October", "November", "December"];
if(type=="single")
{
//var dt=data['actual'];

var dt= data.hasOwnProperty("forecast") ? data['forecast'][0] : data[0];
$(w_block).find("h4").html(dt.district);

dte=f_weather_date(dt.date);
var mydate=new Date(dte.split("-").join("/"));
var temp_month=month[mydate.getMonth()];
var year=mydate.getFullYear()+"";
//$(w_block).find(".date").html("TODAY, "+temp_month+" "+year.substring(2,4));
var date_today = mydate.getDate()+"";
$(w_block).find(".date").html("TODAY, "+temp_month+" "+date_today);
$(w_block).find(".weather-icon i").attr("class",dt.icon);
$(w_block).find(".weather-icon i").attr("title",dt.description);
$(w_block).find(" span.max").html(convert(dt.temp_max));
$(w_block).find("span.min").html(convert(dt.temp_min));
$(w_block).show("fast");
}
else
{
var temp="";
for(var i=1;i<data.length;i++){
dte=f_weather_date(data[i].date);
var mydate=new Date(dte.split("-").join("/"));
var temp_month=month[mydate.getMonth()];
var year=mydate.getFullYear()+"";

//temp +='<li class="clearfix"><span class="date">'+data[i].weekday+', '+temp_month.substring(0,3)+' '+year.substring(2,4)+'</span><div class="weather-icon"><i class="'+data[i].icon+'"></i></div><div class="values"><span class="value"><span class="title">MAX</span>  '+convert(data[i].temp_max)+'</span><span class="value"><span class="title">MIN</span>  '+convert(data[i].temp_min)+'</span></div></li>';
var date_today = mydate.getDate()+"";
//temp +='<li class="clearfix"><span class="date">'+data[i].weekday+', '+temp_month.substring(0,3)+' '+year.substring(2,4)+'</span><div class="weather-icon"><i class="'+data[i].icon+'"></i></div><div class="values"><span class="value"><span class="title">MAX</span>  '+convert(data[i].temp_max)+'</span><span class="value"><span class="title">MIN</span>  '+convert(data[i].temp_min)+'</span></div></li>';
temp +='<li class="clearfix"><span class="date">'+data[i].weekday+', '+temp_month.substring(0,3)+' '+date_today+'</span><div class="weather-icon"><i class="'+data[i].icon+'" title="'+data[i].description+'"></i></div><div class="values"><span class="value"><span class="title">MAX</span>  '+convert(data[i].temp_max)+'</span><span class="value"><span class="title">MIN</span>  '+convert(data[i].temp_min)+'</span></div></li>';

}
$(w_block).find(".slider1 .overview").html(temp);
multi_weather();
}
}
