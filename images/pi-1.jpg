function run_carousel(obj){
//var li_width = $(obj).find('ul.list2 li').width();
var li_width = $(obj+' .viewport').width();
var len = $(obj).find('ul.list2 li').length;
$(obj).find('ul.list2').css({'width': li_width * len +'px', 'left':0});
$('.upcoming-events .prev').addClass('disable');
 if(len==1)
 {
    $('.upcoming-events .next').addClass('disable');
 }
$('.upcoming-events').on('click','.next, .prev', function(e){
    e.preventDefault();
    var li_width = $(obj+' .viewport').width();
    var left = '';
    if($(this).hasClass('next')){
        left = Math.ceil($(obj).find('ul.list2').position().left/li_width)*li_width - li_width;
    } else {
        left = Math.ceil($(obj).find('ul.list2').position().left/li_width)*li_width + li_width;
    }

    $(obj).find('ul.list2').css({'left':left});
    if((Math.abs(left - li_width) >= li_width * len) && !$('.upcoming-events .next').hasClass('disable')){
    $('.upcoming-events .next').addClass('disable');
    } else {
    $('.upcoming-events .next').removeClass('disable');
    }
    if((Math.abs(left) <= 0) && !$('.upcoming-events .prev').hasClass('disable')){
       $('.upcoming-events .prev').addClass('disable');
    } else {
       $('.upcoming-events .prev').removeClass('disable');
    }

    return false;
});

}

function run_weatherCarousel(obj)
{
//var li_width = $(obj).find('ul li').outerWidth();
var len = $(obj).find('ul li').length;
var vi_width = $(obj+' .viewport').width()+3;
$(obj).find('ul').css({'width': vi_width * 3 +'px', 'left':0});
$(obj+' .prev').addClass('disable');
$(obj+' .next').removeClass('disable');
}

$('#slider1').on('click','.next, .prev', function(e)
{
    e.preventDefault();
    var set_left, pos_left, vi_width;

    pos_left = $('#slider1').find('ul').position().left;
    vi_width=  $('#slider1 .viewport').width()+3;

    if($(this).hasClass('next')){
        set_left = Math.ceil(Math.round(pos_left)/vi_width)*vi_width - vi_width;
    } else {
        set_left = Math.ceil(Math.round(pos_left)/vi_width)*vi_width + vi_width;
    }

    $('#slider1').find('ul').css({'left':set_left});
    if((Math.abs(set_left - vi_width) >= vi_width * 3) && !$('#slider1 .next').hasClass('disable')){
    $('#slider1 .next').addClass('disable');
    } else {
    $('#slider1 .next').removeClass('disable');
    }
    if((Math.abs(set_left) <= 0) && !$('#slider1 .prev').hasClass('disable')){
       $('#slider1 .prev').addClass('disable');
    } else {
       $('#slider1 .prev').removeClass('disable');
    }

});



    
                    
                    
